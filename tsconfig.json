{
   "compilerOptions":{
      "allowJs":true,
      "allowSyntheticDefaultImports":true,
      "esModuleInterop":true,
      "isolatedModules":true,
      "jsx":"react-native",
      "lib":[
         "es2017"
      ],
      "moduleResolution":"node",
      "noEmit":true,
      "strict":true,
      "target":"esnext",
      "module":"esnext",
      "experimentalDecorators":true,
      "baseUrl":"./",
      "paths":{
         "src/*":[
            "./src/*"
         ]
      }
   },
   "exclude":[
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
   ],
   "include": ["index.js", "src"],
   "extends":"@tsconfig/react-native/tsconfig.json"
 }